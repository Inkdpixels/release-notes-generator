{
  "name": "@inkdpixels/release-notes-generator",
  "version": "0.0.2",
  "description": "A release-notes-generator plugin for semantic-release.",
  "main": "dist/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "build": "babel src -d dist",
    "lint": "xo src/**/*.js",
    "mocha": "find ./src -name '*.spec.js' | xargs istanbul cover -x *.spec.js _mocha -- -R spec --compilers js:babel-register",
    "test": "npm run lint && npm run mocha && npm run build && node dist/index.js"
  },
  "author": "Tyll Wei√ü <inkdpixels@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.3.17",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.3.13",
    "chai": "^3.4.1",
    "istanbul": "^0.4.1",
    "mocha": "^2.3.4",
    "xo": "^0.12.1"
  },
  "dependencies": {
    "concat-stream": "^1.5.1",
    "conventional-changelog": "^0.5.1",
    "github-url-from-git": "^1.4.0"
  },
  "xo": {
    "esnext": true,
    "envs": [
      "node",
      "mocha",
      "es6"
    ]
  }
}
